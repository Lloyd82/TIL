package test;

public class Test02 {

  public static void main(String[] args) {
    double[] num = { 1.5, 2.7, 4.1, -1.5, 3.3 };
    double min = 9999;
    double max = 0.0;

    for (int i = 0; i < num.length; i++) {
      if (min > num[i])
        min = num[i];
      if (max < num[i])
        max = num[i];
    }
    System.out.println(min);
    System.out.println(max);
  }
}

