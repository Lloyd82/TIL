package test;

import java.util.Calendar;
import java.util.Scanner;

public class Test04 {

  public static void main(String[] args) {
    Scanner s = new Scanner(System.in);
    System.out.println("이름, 전화번호, 주소, 주민번호");
    System.out.println("이름 : "+ s.next());
    System.out.println("전번 : "+ s.next());
    System.out.println("주소 : "+ s.next());
    
    String jumin= args[3]; //921008-2078815 
    int iage = Integer.parseInt(jumin.substring(0,2)); 
    iage += 1900; //1992
    
    Calendar calendar = Calendar.getInstance(); 
    int currYear= calendar.get(Calendar.YEAR); 
    
    System.out.println("나이 : " + (currYear - iage) );
    
    
  }

}
