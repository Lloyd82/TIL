package javastudy.day02;
public class DoWhile{ 
    public static void main(String args[]){ 
         
        int j=1; 

        do{  
            System.out.println("번호 : " + j); //1 
            j++;  //2 
            if(j == 5) break; 
        //j가 0보다 작을 동안 실행     
        }while(j < 0); 

        System.out.println("--------- END ---------"); 
        System.out.println("do-while문 종료후의 j의 값:" + j); 
    } 

}; 