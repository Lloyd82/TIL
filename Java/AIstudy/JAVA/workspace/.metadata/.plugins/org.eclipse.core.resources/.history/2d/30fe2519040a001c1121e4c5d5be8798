package day07;
import java.util.ArrayList; 

class Jumsu{ 
    //멤버 변수 
  String name = "";
  int kuk = 0;
  int eng = 0;
  int tot = 0;
  int avg = 0;
  
  public Jumsu(String name, int kuk, int eng, int tot, int avg) {
    //super(); //있건 없건 상관없기애 삭제 가능 
    this.name = name;
    this.kuk = kuk;
    this.eng = eng;
    this.tot = (kuk+eng);
    this.avg = (kuk+eng) / 2;
  }
    
  //생성자