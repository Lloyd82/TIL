package javastudy.day01;

public class VariableExemple {

  public static void main(String[] args) {
    //10을 변수 value의 초기값으로 저장
    int value = 10;
    //변수 value 값을 읽고 10을 더하는 산술 연산을 수행
    //연산의 결과값을 result의 초기값으로 사용
    int result = value + 10;
    
    //변수 result 값을 읽고 콘솔에 출력
    System.out.println(value +"+"+"10" +"+"="+result);

  }

}
